<h2>New House</h2>

<%= form_for @house do |f| %>
  <p>
    <%= f.label "#{:name}:" %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label "#{:img_url}:" %>
    <%= f.text_field :img_url %>
  </p>
  <p>
    <%= f.label "#{:motto}:" %>
    <%= f.text_field :motto %>
  </p>
  <%= f.submit %>
<% end %>
