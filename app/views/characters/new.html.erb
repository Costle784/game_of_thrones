<%= form_for [@house, @character] do |f| %>
<%= f.hidden_field :house_id %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
  </p>
  <p>
    <%= f.label :weapon %>
    <%= f.text_field :weapon %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
