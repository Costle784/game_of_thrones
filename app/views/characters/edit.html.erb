<%= form_for @character do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
  </p>
  <p>
    <%= f.label :weapon %>
    <%= f.text_field :weapon %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
